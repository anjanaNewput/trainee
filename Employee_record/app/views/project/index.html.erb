<div class="master">
  <h1>Listing projects</h1>
  <p><%= link_to 'New project', new_project_path %></p>
  <table>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3">Options</th>
    </tr>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.description %></td>
        <td><%= link_to 'Show', project_path(project) %></td>
      </tr>
    <% end %>
  </table>
</div>