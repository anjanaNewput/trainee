
  <div class="col-md-9">
    <div class="content-right">
      <div class="page-header"><h2>List Of Employee</h2></div>
      <table>
        <tr>
          <th>Name</th>
          <th>Age</th>
          <th>Email</th>
          <th>Role</th>
          <th>Project_Name</th>
          <th colspan="3">Options</th>
        </tr>
        <% @developers.each do |developer| %>
        <tr>
          <td><%= developer.employee_name %></td>
          <td><%= developer.age %></td>
          <td><%= developer.email %></td>
          <td><%= developer.role %></td>
          <td><%= get_Project_name(developer.project_id) %></td>
          <td><%= link_to 'Show', developer_path(developer) %></td>
          <td><%= link_to 'Edit', edit_developer_path(developer) %></td>
          <td><%= link_to 'Remove', delete_developer_path(developer),method: :delete,
            data: { confirm: 'Are you sure?' } %></td>
        </tr>
        <% end %>
      </table>
    </div>
  </div>
  </div>
</div>
    
