<div class="master">
  <h1> All the Employee </h1>
  <p><%= link_to 'New Employee', new_employee_path %></p>
  <p><%= link_to 'Get Developer', get_developer_path %></p>
  <table>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th>Role</th>
      <th colspan="3">Options</th>
    </tr>
    <% @employees.each do |employee| %>
      <tr>
        <td><%= employee.name %></td>
        <td><%= employee.age %></td>
        <td><%= employee.role %></td>
        <td><%= link_to 'Show', employee_path(employee) %></td>
        <td><%= link_to 'Edit', edit_employee_path(employee) %></td>
        <td><%= link_to 'Delete',delete_employee_path(employee), method: :delete %></td>
      </tr>
    <% end %>
  </table>
</div>